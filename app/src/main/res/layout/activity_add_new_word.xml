<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AddNewWord">

    <EditText
        android:id="@+id/editTextSwedish"
        android:layout_width="224dp"
        android:layout_height="91dp"
        android:layout_marginTop="112dp"
        android:ems="10"
        android:hint="Svenska"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextEnglish"
        android:layout_width="210dp"
        android:layout_height="82dp"
        android:layout_marginTop="60dp"
        android:ems="10"
        android:hint="engelska"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextSwedish" />

    <Button
        android:id="@+id/button2"
        android:layout_width="137dp"
        android:layout_height="73dp"
        android:layout_marginTop="68dp"
        android:text="Add"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextEnglish" />
</androidx.constraintlayout.widget.ConstraintLayout>